  <!-- SIDE SCAN SONAR -->
  <gazebo reference="side_scan_sonar_left_link">
    <sensor type="gpu_ray" name="sonar_left">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>2</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>60</samples>
            <resolution>1</resolution>
            <min_angle>-${0.5*pi/3.0}</min_angle>
            <max_angle>${0.5*pi/3.0}</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>1.0</min>
          <max>200.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.027</stddev>
        </noise>
      </ray>
      <plugin name="sonar_left_controller" filename="libgazebo_ros_gpu_laser.so">
        <topicName>sss_left</topicName>
        <frameName>side_scan_sonar_left_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <gazebo reference="side_scan_sonar_right_link">
    <sensor type="gpu_ray" name="sonar_right">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>2</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>60</samples>
            <resolution>1</resolution>
            <min_angle>-${0.5*pi/3.0}</min_angle>
            <max_angle>${0.5*pi/3.0}</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>1.0</min>
          <max>200.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.027</stddev>
        </noise>
      </ray>
      <plugin name="sonar_right_controller" filename="libgazebo_ros_gpu_laser.so">
        <topicName>sss_right</topicName>
        <frameName>side_scan_sonar_right_link</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <!-- PLUGINS -->
  <!-- <gazebo>
    <plugin name="BuoyancyPlugin" filename="libBuoyancyPlugin.so">
      <fluid_density>1000</fluid_density>
    </plugin>
  </gazebo> -->

  <gazebo>
    <plugin name="uuv_plugin" filename="libuuv_underwater_object_ros_plugin.so">
      <fluid_density>1028</fluid_density>
      <flow_velocity_topic>hydrodynamics/current_velocity</flow_velocity_topic>
      <debug>true</debug>
      <link name="$(arg namespace)/base_link">
        <neutrally_buoyant>0</neutrally_buoyant>
        <volume>1.83826</volume>
        <box>
          <width>1.5</width>
          <length>2.6</length>
          <height>1.6</height>
        </box>
        <center_of_buoyancy>0.0 0.0 0.3</center_of_buoyancy>
        <hydrodynamic_model>
          <type>fossen</type>
          <added_mass>
               779.79 -6.8773 -103.32  8.5426 -165.54 -7.8033
              -6.8773    1222   51.29  409.44 -5.8488  62.726
              -103.32   51.29  3659.9  6.1112 -386.42  10.774
               8.5426  409.44  6.1112   534.9 -10.027  21.019
              -165.54 -5.8488 -386.42 -10.027  842.69 -1.1162
              -7.8033  62.726  10.775  21.019 -1.1162  224.32
          </added_mass>
          <linear_damping>
              -74.82 -69.48 -728.4 -268.8 -309.77 -105
          </linear_damping>
          <quadratic_damping>
              -748.22 -992.53 -1821.01 -672 -774.44 -523.27
          </quadratic_damping>
        </hydrodynamic_model>
      </link>
    </plugin>
  </gazebo>

