<?xml version="1.0"?>

<robot name="first" xmlns:joint="http://playerstage.sourceforge.net/gazebo/xmlschema/#joint" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find robot-lab)/gazebo/default.gazebo" />

  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.05" />
      <geometry>
        <box size="0.5 0.25 0.15" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.05" />
      <geometry>
        <box size="0.5 0.25 0.15" />
      </geometry>
    </collision>
    <inertial>
      <mass value="5" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.2" ixy="0.0" ixz="0.0" iyy="0.2" iyz="0.0" izz="0.2" />
    </inertial>
  </link>

  <link name="front_link">
    <visual>
      <geometry>
        <box size="0.015 0.22 0.005" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.015 0.22 0.005" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="back_link">
    <visual>
      <geometry>
        <box size="0.015 0.22 0.01" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.015 0.22 0.01" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.01" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="front_left_bar_link">
    <visual>
      <origin xyz="-0.02 0 0" />
      <geometry>
        <box size="0.03 0.01995 0.005" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.02 0 0" />
      <geometry>
        <box size="0.03 0.01995 0.005" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="front_right_bar_link">
    <visual>
      <origin xyz="-0.02 0 0" />
      <geometry>
        <box size="0.03 0.01995 0.005" />
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.02 0 0" />
      <geometry>
        <box size="0.03 0.01995 0.005" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.2" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="front_left_wheel_link">
    <visual>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.06" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.025" radius="0.06" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="front_right_wheel_link">
    <visual>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.06" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.025" radius="0.06" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="back_left_wheel_link">
    <visual>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.06" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.025" radius="0.06" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="back_right_wheel_link">
    <visual>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.05" radius="0.06" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="1.57079632679 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.025" radius="0.06" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.4" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01" />
    </inertial>
  </link>

  <link name="cabin">
    <visual>
      <geometry>
        <sphere radius="0.1" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.1" />
      </geometry>
    </collision>
    <inertial>
      <mass value="2" />
      <inertia ixx="0.2" ixy="0.0" ixz="0.0" iyy="0.2" iyz="0.0" izz="0.2" />
    </inertial>
  </link>

  <!-- JOINTS -->
  <joint name="front_bar_joint" type="fixed">
    <parent link="base_link" />
    <child link="front_link" />
    <origin xyz="0.16 0.0 -0.1" />
  </joint>

  <joint name="back_bar_joint" type="fixed">
    <axis xyz="0 0 1" />
    <parent link="front_link" />
    <child link="back_link" />
    <origin rpy="0 0 0" xyz="-0.04 0 0" />
  </joint>

  <joint name="front_left_bar_joint" type="revolute">
    <limit effort="100" lower="-0.5" upper="0.5" velocity="1" />
    <axis xyz="0 0 1" />
    <parent link="front_link" />
    <child link="front_left_bar_link" />
    <origin rpy="0 0 0" xyz="0.0 0.1 0.0" />
  </joint>

  <joint name="front_right_bar_joint" type="revolute">
    <limit effort="100" lower="-0.5" upper="0.5" velocity="1" />
    <axis xyz="0 0 1" />
    <parent link="front_link" />
    <child link="front_right_bar_link" />
    <origin rpy="0 0 0" xyz="0.0 -0.1 0.0" />
  </joint>

  <joint name="front_left_wheel_joint" type="continuous">
    <axis xyz="0 1 0" />
    <parent link="front_left_bar_link" />
    <child link="front_left_wheel_link" />
    <origin rpy="0 0 0" xyz="0 0.06 0" />
  </joint>

  <joint name="front_right_wheel_joint" type="continuous">
    <axis xyz="0 1 0" />
    <parent link="front_right_bar_link" />
    <child link="front_right_wheel_link" />
    <origin rpy="0 0 0" xyz="0 -0.06 0" />
  </joint>

  <joint name="back_left_wheel_joint" type="continuous">
    <axis xyz="0 1 0" />
    <parent link="base_link" />
    <child link="back_left_wheel_link" />
    <origin rpy="0 0 0" xyz="-0.16 0.155 -0.1" />
  </joint>

  <joint name="back_right_wheel_joint" type="continuous">
    <axis xyz="0 1 0" />
    <parent link="base_link" />
    <child link="back_right_wheel_link" />
    <origin rpy="0 0 0" xyz="-0.16 -0.155 -0.1" />
  </joint>

  <joint name="cabin_joint" type="fixed">
    <parent link="base_link" />
    <child link="cabin" />
    <origin xyz="0 0 0.13" />
  </joint>

  <!-- MATERIAL -->

  <gazebo reference="base_link">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="cabin">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="front_link">
    <material>Gazebo/Green</material>
  </gazebo>

  <gazebo reference="back_link">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="front_left_bar_link">
    <material>Gazebo/Green</material>
  </gazebo>

  <gazebo reference="front_right_bar_link">
    <material>Gazebo/Green</material>
  </gazebo>

  <gazebo reference="front_right_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="front_left_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="back_left_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="back_right_wheel_link">
    <material>Gazebo/Black</material>
  </gazebo>

</robot>